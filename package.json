{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "description": "Basic React SSR setup with Express",
  "main": "index.js",
  "scripts": {
    "start": "npm run server-side-react",
    "dev": "npx webpack --watch --config webpack.config.dev.js --mode development",
    "server-side-react": "nodemon src/server.js --exec babel-node --presets @babel/env,@babel/react",
    "sass-watch": "sass --style=compressed --watch ./src/scss/index.scss ./public/css/styles.min.css",
    "build": "npx webpack --config webpack.config.prod.js --mode production",
    "build-watch": "npx webpack --watch --config webpack.config.prod.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reyarqueza/isomorphic-react.git"
  },
  "author": "Rey Arqueza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reyarqueza/isomorphic-react/issues"
  },
  "homepage": "https://github.com/reyarqueza/isomorphic-react#readme",
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.9",
    "@graphql-tools/webpack-loader": "^6.6.2",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "apicache": "^1.6.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-import": "^1.13.5",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "express-ip": "^1.0.4",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "mongodb": "^4.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-query": "^3.34.20",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
